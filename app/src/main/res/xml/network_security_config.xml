<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">172.16.6.150</domain>
        <domain includeSubdomains="true">www.gofinito.com</domain>
        <domain includeSubdomains="true">10.0.2.2:8088</domain>
    </domain-config>
    <base-config cleartextTrafficPermitted="true">
